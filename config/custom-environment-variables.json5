{
  strategy: 'RIFS_STRATEGY',
  db: 'RIFS_DB',
  comms: {
    libp2p: {
      addresses: {
        listen: {
          "__name": "RIFS_COMMS_LISTEN",
          "__format": "json"
        }
      },
      config: {
        peerDiscovery: {
          bootstrap: {
            enabled: "RIFS_COMMS_BOOTSTRAP_ENABLED",
            list: {
              "__name": "RIFS_COMMS_BOOTSTRAP_LIST",
              "__format": "json"
            }
          }
        }
      }
    },
  },
  blockchain: {
    provider: 'RIFS_PROVIDER',
    contractAddress: 'RIFS_CONTRACT_ADDR'
  },
  log: {
    level: 'LOG_LEVEL',
    filter: 'LOG_FILTER',
    path: 'LOG_PATH'
  }
}
