{
  "name": "@rsksmart/rif-storage-ipfs-pinning",
  "description": "Application for providing your storage space to other to use in exchange of RIF Tokens",
  "version": "0.1.0",
  "author": "Adam Uhlíř @AuHau",
  "bin": {
    "rif-pinning": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/rsksmart/rds-ipfs/issues/"
  },
  "contributors": [
    "Adam Uhlíř <adam@uhlir.dev>"
  ],
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/errors": "^1.3.1",
    "@oclif/parser": "^3.8.5",
    "@oclif/plugin-help": "^3.1.0",
    "@rsksmart/rif-marketplace-storage": "^0.1.0-dev.0",
    "async-sema": "^3.1.0",
    "bn.js": "^5.1.2",
    "cids": "^0.8.3",
    "cli-ux": "^5.4.6",
    "config": "^3.3.1",
    "ipfs-http-client": "^44.3.0",
    "multiaddr": "^7.5.0",
    "oclif": "^1.16.1",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.2",
    "sequelize": "^5.22.1",
    "sequelize-store": "^0.3.1",
    "sequelize-typescript": "^1.1.0",
    "sql-formatter": "^2.3.3",
    "sqlite3": "^4.2.0",
    "web3-core-subscriptions": "^1.2.9",
    "web3-eth": "^1.2.9",
    "web3-eth-contract": "^1.2.9",
    "web3-utils": "^1.2.9",
    "winston": "^3.3.3",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@fluffy-spoon/substitute": "^1.143.0",
    "@oclif/dev-cli": "^1.22.2",
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/config": "0.0.36",
    "@types/dirty-chai": "^2.0.2",
    "@types/mocha": "^7.0.2",
    "@types/multiaddr": "^7.2.0",
    "@types/node": "^14.0.14",
    "@types/semver": "^7.3.1",
    "@types/sinon": "^9.0.3",
    "@types/sinon-chai": "^3.2.4",
    "@types/sql-formatter": "^2.3.0",
    "@types/validator": "^13.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.2",
    "dirty-chai": "^2.0.1",
    "globby": "^11.0.1",
    "ipfs": "^0.40.0",
    "ipfsd-ctl": "^0.47.4",
    "merge-options": "^2.0.0",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "tasegir": "^1.7.1",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "lib",
    "types"
  ],
  "homepage": "https://github.com/rsksmart/rds-ipfs",
  "keywords": [
    "IPFS",
    "Pinning",
    "RIF"
  ],
  "license": "MIT",
  "main": "src/cli.ts",
  "oclif": {
    "bin": "rif-pinning",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "https://github.com/rsksmart/rds-ipfs.git",
  "scripts": {
    "bin": "tasegir run --watch ./bin/run -- ",
    "compile": "tasegir compile",
    "compile:watch": "tasegir compile -- --watch",
    "docs": "typedoc --mode modules --excludeNotExported --readme none --excludePrivate  --tsconfig ./node_modules/tasegir/src/config/tsconfig.json --exclude 'src/providers/*,test/**/*' --out docs src",
    "exec": "tasegir run",
    "init": "./.repos/init.sh",
    "ipfs:consumer": "cross-env IPFS_PATH='./.repos/consumer' ipfs",
    "ipfs:provider": "cross-env IPFS_PATH='./.repos/provider' ipfs",
    "lint": "tasegir lint",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "tasegir run --watch node_modules/.bin/oclif-dev -- manifest",
    "release": "tasegir release --no-build",
    "start": "tasegir run --watch ./bin/run --",
    "test": "tasegir test --target node",
    "types-check": "tasegir types-check",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "types/index.d.ts"
}
